{
    "files.exclude": {
        "**/.*.*.cmd": true,
        "**/.*.d": true,
        "**/.*.S": true,
        "**/.*.tmp": true,
        "**/*.a": true,
        "**/*.dtb": true,
        "**/*.ko": true,
        "**/*.mod": true,
        "**/*.mod.c": true,
        "**/*.o": true,
        "**/modules.order": true,
    },
    "[c]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 8,
        "editor.insertSpaces": false,
        // "editor.rulers": [
        //     80,
        //     100
        // ]
    },
    "files.associations": {
        "*.h": "c"
    }
}